html,body,p,ol,ul,li,dl,dt,dd,blockquote,figure,fieldset,legend,textarea,pre,iframe,hr,h1,h2,h3,h4,h5,h6
  margin 0
  padding 0

h1,h2,h3,h4,h5,h6
  font-size 100%
  font-weight normal

ul, li
  list-style none

button,input,select,textarea
  margin 0

html
  box-sizing border-box

*, *before, *after
  box-sizing inherit

img,embed,iframe,object,audio,video
  height auto
  max-width 100%

iframe
  border 0

table
  border-collapse collapse
  border-spacing 0

td,th
  padding 0
  text-align left
.icon
  width: 1em;
  height: 1em;
  vertical-align: -0.15em;
  fill: currentColor;
  overflow: hidden;
  margin-right: 5px;